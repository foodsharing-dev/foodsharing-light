  "scripts": {
    "clean": "node build/script.clean.js",
    "dev": "node build/script.dev.js",
    "build": "node build/script.build.js",
    "lint": "eslint --ext .js,.vue src",
    "test": "cross-env BABEL_ENV=test NODE_ENV=test karma start test/unit/karma.conf.js --single-run",
    "test:xvfb": "if command -v xvfb-run >/dev/null 2>&1; then xvfb-run npm run test; else echo xvfb-run command is not available; fi",
    "test:watch": "cross-env BABEL_ENV=test NODE_ENV=test karma start test/unit/karma.conf.js",
    "test:watch:xvfb": "if command -v xvfb-run >/dev/null 2>&1; then xvfb-run npm run test:watch; else echo xvfb-run command is not available; fi"
